  "private": true,
  "browserify": {
    "transform": [
      "envify",
      "unassertify",
      "es2020",
      "uglifyify"
    ]
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:css": "node-sass --output-style compressed styles/styles.sass > build/styles.css",
    "build:licenses": "brlicenses -f full index.js > build/LICENSES",
    "build:js": "browserify -p [ browserify-header --file header.js ] -p licensify index.js | uglifyjs --comments warnings=false > build/browser.js",
    "chromedriver": "chromedriver --url-base=/wd/hub",
    "start": "npm-run-all --parallel start:*",
    "start:api": "PORT=8081 LEVELDB=memory ADMINISTRATOR_PASSWORD=test commonform-server",
    "start:app": "PORT=8000 node server.js",
    "start:db": "PORT=4444 BLOBS=memory LEVELDOWN=memory tcp-log-server",
    "start:proxy": "PORT=8080 node proxy.js",
    "test": "tape test/*.test.js",
    "lint": "standard",
    "licensee": "licensee 2>&1",
    "precommit": "lint-staged",
    "watch:css": "node-sass --watch --source-map true --output build styles/styles.sass",
    "watch:js": "watchify --ignore-transform uglifyify index.js -o build/browser.js -dv"
  },
  "lint-staged": {
    "*.js": "standard"
  }
}
