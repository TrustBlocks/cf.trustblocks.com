{
  "type": "object",
  "additionalProperties": false,
  "required": [
    "edition",
    "published"
  ],
  "properties": {
    "title": {
      "type": "string",
      "minLength": 3
    },
    "edition": {
      "type": "string",
      "minLength": 2
    },
    "published": {
      "type": "string",
      "format": "date-time"
    },
    "numbering": {
      "enum": [
        "outline",
        "decimal"
      ]
    },
    "notes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "signaturePages": {
      "type": "array"
    }
  }
}
